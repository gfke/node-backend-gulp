{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": ".*(node_modules|[.]git)|.+\\.spec.js$"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false,
    "linenums": true,
    "collapseSymbols": false,
    "default": {
      "outputSourceFiles": false
    }
  },
  "opts": {
    "recurse": true,
    "private": true
  },
  "plugins": ["plugins/markdown"],
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  }
}